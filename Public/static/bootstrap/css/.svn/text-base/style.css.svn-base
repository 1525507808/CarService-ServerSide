body{
	background-color: #fff;
}
.top-nav{
	display:block;
	margin-right: 50px;
}
.top-nav li{
	display:inline-block;
	list-style-type: none;
	margin-top:5px;
	margin-left: 15px;
}
.top-nav li a{
	color: #999;
}
.top-nav li a:hover,
.top-nav li a:focus
{
	color:#f87d22;
	text-decoration: none;
}
.msgCount{
	color:#ff0000; 
}
.navbar-default .navbar-nav{
	margin-left: -70px;
}
.navbar-default .navbar-nav > li > a, .navbar-default .navbar-nav > li > a{
	font-size: 18px;
	color: #ffffff;
}
.navbar-default .navbar-nav > li > a:hover, 
.navbar-default .navbar-nav > li > a:focus, 
.navbar-default .navbar-nav > li > a.active{
	color: #ffffff;
	opacity: 0.7;
	background-color: rgba(0,0,0,0.3);
	border-bottom: 0px;
}
.navbar-default .navbar-nav > .open > a,
.navbar-default .navbar-nav > .open > a:hover,
.navbar-default .navbar-nav > .open > a:focus{
	color: #ffffff;
	opacity: 0.7;
	background-color: rgba(0,0,0,0.3);
	border-bottom: 0px;
}
.bread-nav{
	display:block;
	margin-right: 130px;
}
.bread-nav li{
	display:inline-block;
	margin-top:0px;
	margin-left: 15px;
	font-size: 16px;
}
.bread-nav li.active{
	color: #f87d22;
	border-bottom: 2px solid #f87d22;
}
.bread-nav li a{
	color: #999;
	padding: 3px 5px;
}
.bread-nav li a:hover,
.bread-nav li a:focus,
.bread-nav li a.active{
	text-decoration: none;
	color: #f87d22;
	border-bottom: 2px solid #f87d22;
}
#ycbb-project-list-panel{
	padding-left:0px; 
	padding-right:0px; 
}
.table > thead > tr > th{
	border-bottom: 0px;
	padding:15px 5px;
}
#project-table thead tr th,
#project-table thead tr td,
#project-table tbody tr th,
#project-table tbody tr td{
	text-align: center;
}
#project-table thead{
	background-color:#eee;
	color: #999;
	/*
color: #f87d22;
	font-size: 16px;
*/
}
#pagination{
	margin-top: -10px;
}
.bootstrap-dialog .modal-header.bootstrap-dialog-draggable {
    cursor: move;
}
.form-dialog .modal-dialog {
	width: 90%;
}
.bootstrap-dialog .bootstrap-dialog-header{
	text-align: center;
	font-size:20px;
}
.modal-footer{
	text-align: center;
}
input[name="checkbox"]{
	width: 20px;
	height: 20px;
}
.ycbb-star{
	color:rgba(250,200,100,1);
}
.bs-callout-warning {
    border-left-color: #F0AD4E;
}
.bs-callout {
    padding: 20px;
    margin: 20px 0px;
    border-width: 1px 1px 1px 5px;
    border-style: solid;
    border-color: #EEE;
    -moz-border-top-colors: none;
    -moz-border-right-colors: none;
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    border-image: none;
    border-radius: 3px;
}
.bs-user{
	padding:5px 10px;
	font-size: 22px;
	box-shadow: 0px 3px 0px rgba(0,0,0,0.2);
	-webkit-box-shadow: 0px 3px 0px rgba(0,0,0,0.2);
	-moz-box-shadow: 0px 3px 0px rgba(0,0,0,0.2);
}